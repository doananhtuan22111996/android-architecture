<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_material_kit"
    app:startDestination="@id/materialKitFragment">

    <action
        android:id="@+id/action_materialKitFragment_to_typographyFragment"
        app:destination="@id/typographyFragment" />

    <fragment
        android:id="@+id/materialKitFragment"
        android:name="vn.root.app.pages.materialKit.MaterialKitFragment"
        android:label="fragment_material_kit"
        tools:layout="@layout/fragment_material_kit" />

    <fragment
        android:id="@+id/typographyFragment"
        android:name="vn.root.app.pages.materialKit.typography.TypographyFragment"
        android:label="fragment_typography"
        tools:layout="@layout/fragment_typography" />

</navigation>